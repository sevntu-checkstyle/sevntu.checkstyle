	<rule-metadata name="%IllegalCatchExtended.name" internal-name="IllegalCatchExtended" parent="TreeWalker">
            <alternative-name internal-name="com.github.sevntu.checkstyle.checks.coding.IllegalCatchExtendedCheck"/>
